{
  "name": "backend-blog",
  "version": "1.0.0",
  "description": "backend for blog app",
  "main": "index.ts",
  "scripts": {
    "clean": "rm -rf ./dist",
    "type-check": "tsc --noEmit",
    "before-build": "npm run clean && yarn && npm run type-check",
    "build": "npm run before-build && tsc -p .",
    "dev": "nodemon --exec babel-node --extensions \".ts\" ./src/bin/server.ts",
    "serve:development": "NODE_ENV=development node ./dist/bin/server.js",
    "refresh:env-jwt": "node ./src/scripts/generateEnvJWT.ts",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:fresh": "npx sequelize-cli db:migrate:undo:all && npm run db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:drop": "npx sequelize-cli db:drop",
    "db:reset": "npm run db:drop && npx sequelize-cli db:create && npm run db:migrate && npm run db:seed"
  },
  "repository": "https://github.com/muchamadagush/backend-blog",
  "author": "Muchamad Agus Hermawan",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "pug": "^3.0.2",
    "sequelize": "^6.25.5",
    "sequelize-cli": "^6.5.2",
    "slugify": "^1.6.5",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "yup": "^1.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.7",
    "@babel/core": "^7.12.16",
    "@babel/node": "^7.12.16",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.18.9",
    "@babel/runtime": "^7.12.13",
    "@types/bcryptjs": "^2.4.4",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/http-errors": "^2.0.1",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/lodash": "^4.14.188",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.9",
    "@types/uuid": "^8.3.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "import": "^0.0.6",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=16.x"
  }
}
